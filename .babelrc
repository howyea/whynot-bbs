{
  "presets": [
      "stage-2",
      "latest",
      "es2015",
      "es2016",
      "es2017",
      "react",
      "stage-0",
    [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": "> 1%",
            "uglify": true
          },
          "useBuiltIns": true
        }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
      "transform-class-properties",
    "babel-plugin-styled-components"
  ]
}
